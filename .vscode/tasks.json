// See https://go.microsoft.com/fwlink/?LinkId=733558
// for the documentation about the tasks.json format
{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "npmwatch",
			"type": "npm",
			"script": "watch",
			"problemMatcher": "$tsc-watch",
			"isBackground": true,
			"presentation": {
				"reveal": "never"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "restartcomp",
			"command": "${command:restart-computer.restartComputer}",
			"args": ["audioCues.help"]
		},
		{
			"label": "restartcomxp",
			"command": "${command:workbench.action.tasks.runTask}",
			"problemMatcher":[]
		}
	]
}
